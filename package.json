{
  "name": "for-hrs-project-with-webpack",
  "version": "1.0.0",
  "main": "app.js",
  "author": "masayuki suzuki",
  "license": "MIT",
  "scripts": {
    "imagemin": "imagemin assets/img/original/*.* --out-dir=./www/jobfind-pc/original/images/ --plugin=pngquant",
    "themeImgMin": "imagemin -p pngquant assets/img/theme/*.* -o www/jobfind-pc/themes/default/images",
    "watch-imagemin": "onchange 'assets/img/original/*' -- npm run imagemin",
    "dev": "webpack --config ./config/webpack.config.js --watch --progress",
    "server": "browser-sync start --config ./config/bs-config.js",
    "start": "npm-run-all -p dev watch-imagemin server",
    "build": "webpack --config ./config/webpack.config.js",
    "prod": "webpack --env.prod --config ./config/webpack.config.js",
    "remover": "rm -rf www/jobfind-pc/original/js/original.js www/jobfind-pc/original/js/original.js.map www/jobfind-pc/original/js/topPage.js www/jobfind-pc/original/js/topPage.js.map www/jobfind-pc/original/js/normalize.js www/jobfind-pc/original/js/normalize.js.map www/jobfind-pc/original/js/vendor"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.23.6",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "imagemin": "^5.3.1",
    "imagemin-cli": "^3.0.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "node-sass": "^4.7.2",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "pug": "^2.0.1",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "swiper": "^4.1.6"
  }
}
